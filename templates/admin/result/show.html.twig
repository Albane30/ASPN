{% extends 'base.html.twig' %}

{% block title %}Result{% endblock %}

{% block body %}
    <h1>Result</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ result.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ result.date ? result.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ result.content }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_R_result_index') }}">back to list</a>

    <a href="{{ path('admin_R_result_edit', {'id': result.id}) }}">edit</a>

    {{ include('admin/result/_delete_form.html.twig') }}
{% endblock %}
